        compatible = "zmk,keymap";

    // default layer matches printing on keyboard

default_layer {
 bindings = <
  &kp ESC &kp F1 &kp F2 &kp F3 &kp F4 &kp F5 &kp F6 &kp F7 &kp F8 &kp F9 &kp F10 &kp F11  &kp F12 &kp PSCRN &kp SLCK &kp F24 &tog 1 &mo 2
  &kp GRAVE &kp N1    &kp N2   &kp N3    &kp N4    &kp N5          &kp N6   &kp N7   &kp N8    &kp N9   &kp N0   &kp BACKSPACE                                                             
  &kp TAB   &kp Q     &kp W    &kp E     &kp R     &kp T           &kp Y    &kp U    &kp I     &kp O    &kp P    &kp BSLH                                                              
  &mt CLCK ESC  &kp A     &kp S    &kp D     &kp F     &kp G           &kp H    &kp J    &kp K     &kp L    &kp SEMI &kp SQT                                                               
  &mt LSHFT LPAR &kp Z     &kp X    &kp C     &kp V     &kp B           &kp N    &kp M    &kp COMMA &kp DOT  &kp FSLH &mt RSHFT RPAR                                                             
